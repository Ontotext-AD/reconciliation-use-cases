{
  "places": {
    "indexName": "places",
    "indexType": "semantic-index",
    "elasticUrl": "http://elastic:9200",
    "queryField": [
      "placeAltLabel",
      "placeLabel"
    ],
    "nameField": "prefLabel",
    "typeField": "type",
    "typeFieldBoost": 2,
    "searchFieldPhraseBoost": 1,
    "searchFieldPhraseSlop": 2,
    "searchFieldBoost": 1.2,
    "types": [
      "http://www.geonames.org/ontology#Feature"
    ],
    "defaultType": "http://www.geonames.org/ontology#Feature",
    "propertiesBoost": {
      "parentADM1": 0.7,
      "parentADM1Alt": 0.5,
      "parentADM2": 0.7,
      "parentADM2Alt": 0.5,
      "parentCountry": 0.7,
      "parentCountryAlt": 0.5
    },
    "preview": {
      "mode": "EXTERNAL",
      "urlTemplate": "https://wikidata.reconci.link/en/preview?id={{id}}",
      "idRegex": ".*/([^/]*)$"
    }
  }
}